{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/token",
      "destination": "/api/token"
    },
    {
      "source": "/assets/:path*",
      "destination": "/dist/client/assets/:path*"
    },
    {
      "source": "/entry-client.js",
      "destination": "/dist/client/entry-client.js"
    },
    {
      "source": "/entry-server.js",
      "destination": "/dist/client/entry-server.js"
    },
    {
      "source": "/base.css",
      "destination": "/dist/client/base.css"
    }
  ],
  "buildCommand": "npm run build",
  "framework": "vite",
  "installCommand": "npm install",
  "outputDirectory": "dist/client"
} 