{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/token",
      "destination": "/api/token"
    },
    {
      "source": "/(.*)",
      "destination": "/server/index.js"
    }
  ],
  "buildCommand": "npm run build && ls /vercel/output/",
  "framework": "vite",
  "installCommand": "npm install",
  "outputDirectory": "/vercel/output/dist"
} 