{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/token",
      "destination": "/api/token"
    },
    {
      "source": "/(.*)",
      "destination": "/server/index.js"
    }
  ],
  "buildCommand": "rm -rf dist && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install"
} 